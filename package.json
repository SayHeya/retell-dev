{
  "name": "@retell/cli",
  "version": "1.0.0",
  "description": "CLI tool for managing Retell AI agents across workspaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "retell": "./bin/retell.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "workspaces": [
    "packages/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "require": "./dist/cli/index.js"
    },
    "./templates/*": "./dist/templates/*"
  },
  "scripts": {
    "build": "npm run build:module && tsc -p tsconfig.build.json && cp -r src/templates dist/",
    "build:module": "npm run build -w @heya/retell.controllers",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "dev": "ts-node src/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:cli": "bash tests/cli-test.sh",
    "test:all": "npm run test && npm run test:cli",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2 && npm run test:cli",
    "prepublishOnly": "npm run build",
    "precommit": "npm run lint && npm run type-check && npm run test:unit",
    "create-100-agents": "bash scripts/create-100-agents.sh",
    "create-100-agents:ts": "ts-node scripts/create-100-agents.ts",
    "push-100-agents": "bash scripts/push-agents-with-rate-limit.sh test-agents-100 staging 2"
  },
  "keywords": [
    "retell",
    "ai",
    "voice",
    "agent",
    "cli",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@heya/retell.controllers": "*",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "dotenv": "^17.2.3",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
